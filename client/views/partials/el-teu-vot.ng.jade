#elTeuVotContainer(etv-offset-on-element="header")
    p.text-center#countText(ng-if="count > 0")
        span.text-bold.text-orange {{ count }}&nbsp;&nbsp;
        span(translate="countText")
    p#welcomeText(translate="welcomeText")

    //p#soonText.text-uppercase(translate="{{ 'soonText' }}")
    button.etvButton(ng-click="goToPoliticalQuestions()")
        span.text-uppercase(translate="welcomeStart")

    button.etvButton#exampleButton(ng-click="goToExample()")
        span.text-uppercase(translate="example")